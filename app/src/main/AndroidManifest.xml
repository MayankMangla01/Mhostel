<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vampcat.hostelm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SuccessfulLogin"></activity>
        <activity android:name=".ForgotPassword"></activity>
        <activity android:name=".Registration"></activity>
        <activity android:name="Complaints"></activity>
        <activity android:name="Notifications"></activity>
        <activity android:name=".MessMenu"></activity>
        <activity android:name=".Mhostel"></activity>
    </application>

</manifest>